&tlmm {
	cam_sensor_mipi_switch_default: cam_sensor_mipi_switch_default {
		/* RESET, STANDBY */
		mux {
			pins = "gpio97", "gpio96";
			function = "gpio";
		};

		config {
			pins = "gpio97","gpio96";
			bias-disable; /* No PULL */
			drive-strength = <2>; /* 2 MA */
		};
	};

	cam_sensor_mipi_switch_sleep: cam_sensor_mipi_switch_sleep {
		/* RESET, STANDBY */
		mux {
			pins = "gpio97","gpio96";
			function = "gpio";
		};

		config {
			pins = "gpio97","gpio96";
			bias-disable; /* No PULL */
			drive-strength = <2>; /* 2 MA */
		};
	};

	cam_sensor_vio: cam_sensor_vio {
		/* VIO */
		mux {
			pins = "gpio99";
			function = "gpio";
		};

		config {
			pins = "gpio99";
			bias-disable; /* No PULL */
			drive-strength = <2>; /* 2 MA */
		};
	};

	cam_sensor_vio_sleep: cam_sensor_vio_sleep {
		/* VIO */
		mux {
			pins = "gpio99";
			function = "gpio";
		};

		config {
			pins = "gpio99";
			bias-disable; /* No PULL */
			drive-strength = <2>; /* 2 MA */
		};
	};
};

&cam_sensor_rear_reset {
	mux {
		pins = "gpio38", "gpio35";
	};

	config {
		pins = "gpio38", "gpio35";
	};
};

&cam_sensor_rear_reset_sleep {
