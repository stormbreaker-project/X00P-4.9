		pins = "gpio38", "gpio35";
	};

	config {
		pins = "gpio38", "gpio35";
	};
};

&cam_sensor_front_default {
	mux {
		pins = "gpio36","gpio50";
	};

	config {
		pins = "gpio36","gpio50";
	};
};

&cam_sensor_front_sleep {
	mux {
		pins = "gpio36","gpio50";
	};

	config {
